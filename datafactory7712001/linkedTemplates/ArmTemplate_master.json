{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory7712001"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"appvm_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'appvm'"
		},
		"mydatalakestorage2001_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'mydatalakestorage2001'"
		},
		"mysynapseworkspace7712001_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'mysynapseworkspace7712001'"
		},
		"appvm_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\inetpub\\logs\\LogFiles\\W3SVC1"
		},
		"appvm_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "appvm"
		},
		"mydatalakestorage2001_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mydatalakestorage2001.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory7712001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"mydatalakestorage2001_accountKey": {
						"value": "[parameters('mydatalakestorage2001_accountKey')]"
					},
					"mysynapseworkspace7712001_connectionString": {
						"value": "[parameters('mysynapseworkspace7712001_connectionString')]"
					},
					"mydatalakestorage2001_properties_typeProperties_url": {
						"value": "[parameters('mydatalakestorage2001_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "datafactory7712001_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"appvm_password": {
						"value": "[parameters('appvm_password')]"
					},
					"appvm_properties_typeProperties_host": {
						"value": "[parameters('appvm_properties_typeProperties_host')]"
					},
					"appvm_properties_typeProperties_userId": {
						"value": "[parameters('appvm_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datafactory7712001_ArmTemplate_0')]"
			]
		}
	]
}